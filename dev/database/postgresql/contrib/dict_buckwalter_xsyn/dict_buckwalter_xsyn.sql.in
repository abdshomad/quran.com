-- Adjust this setting to control where the objects get created.
SET search_path = public;

CREATE OR REPLACE FUNCTION dbuckwalter_xsyn_init(internal)
        RETURNS internal
        AS 'MODULE_PATHNAME'
        LANGUAGE C STRICT;

CREATE OR REPLACE FUNCTION dbuckwalter_xsyn_lexize(internal, internal, internal, internal)
        RETURNS internal
        AS 'MODULE_PATHNAME'
        LANGUAGE C STRICT;

CREATE TEXT SEARCH TEMPLATE buckwalter_xsyn_template (
        LEXIZE = dbuckwalter_xsyn_lexize,
	INIT   = dbuckwalter_xsyn_init
);

CREATE TEXT SEARCH DICTIONARY buckwalter_xsyn (
	TEMPLATE = buckwalter_xsyn_template
);

COMMENT ON TEXT SEARCH DICTIONARY buckwalter_xsyn IS 'buckwalter xsyn (case sensitive clone of xsyn)';
